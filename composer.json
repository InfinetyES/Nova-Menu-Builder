{
    "name": "dharmeshshah09/nova-menu-builder",
    "description": "Menu Builder Tool for Laravel Nova.",
    "keywords": [
        "laravel",
        "nova",
        "menu",
        "builder",
        "infinety"
    ],
	 "repositories": [
        {
            "type": "package",
            "url": "https://github.com/dharmeshshah09/Laravel-Nova-Menu-Builder.git"
        }
],
    "license": "MIT",
    "authors": [
        {
            "name": "dharmeshshah09",
            "email": "dharmesh@arhamwebworks.com",
            "role": "Developer"
        },
    ],
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Infinety\\MenuBuilder\\": "src/"
        },
        "files": [
          "src/Http/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "dharmeshshah09\\MenuBuilder\\MenuBuilderServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
